          "_customHTML": "<section class=\"features7 kidsm4_features7\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"15\" step=\"1\" value=\"5\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"15\" step=\"1\" value=\"5\">\n\t\t<select title=\"Cards\" name=\"cardsAmount\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\">2</option>\n\t\t\t<option value=\"3\">3</option>\n\t\t\t<option value=\"4\">4</option>\n\t\t\t<option value=\"5\">5</option>\n\t\t\t<option value=\"6\" selected>6</option>\n\t\t</select>\n\t\t<header>Show/Hide</header>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Card Title\" name=\"showTitle2\" checked>\n\t\t<input type=\"checkbox\" title=\"Card Text\" name=\"showText\" checked>\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"../_images/children.jpg\n          \" parallax>\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://vimeo.com/201169087\">\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n\n\t<div class=\"container-fluid\">\n\t\t<div class=\"row\" mbr-if=\"showTitle || showSubtitle\">\n\t\t\t<div class=\"align-center col-md-12 title\">\n\t\t\t\t<h3 class=\"mbr-section-title mbr-fonts-style line-bg-warning\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title, .mbr-section-title\">\n\t\t\t\t\tConnections</h3>\n\t\t\t\t<h4 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">We follow accepted Standards of Education quality operation and Staff competence.<div><br></div></h4>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"wrapper-row\">\n\t\t\t<div class=\"column-1\">\n\t\t\t\t<div class=\"card card-left\">\n\t\t\t\t\t<div class=\"card-box card-box1\">\n\t\t\t\t\t\t<h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle2\" data-app-selector=\".card-title\">\n\t\t\t\t\t\t\tWAEC</h4>\n\t\t\t\t\t\t<p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">\n\t\t\t\t\t\t\tRegistered with WAEC</p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"item-image\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/waec-160x160.png\" alt>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t<div class=\"card\" mbr-if=\"cardsAmount > 1\">\n\t\t\t\t\t<div class=\"card-box card-box1\">\n\t\t\t\t\t\t<h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle2\" data-app-selector=\".card-title\">\n\t\t\t\t\t\t\tMOE</h4>\n\t\t\t\t\t\t<p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Registered Sokoto State Ministry for Education</p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"item-image\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/site-logo-376x80-1-85x79.png\" alt>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t<div class=\"card card-left\" mbr-if=\"cardsAmount > 2\">\n\t\t\t\t\t<div class=\"card-box card-box1\">\n\t\t\t\t\t\t<h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle2\" data-app-selector=\".card-title\">\n\t\t\t\t\t\t\tNAPST</h4>\n\t\t\t\t\t\t<p class=\"mbr-text last-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">\n\t\t\t\t\t\t\tOur Teachers are members of&nbsp;National Association of Private School Teachers </p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"item-image\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/106400765-122639976158804-1061865401983523929-n-160x159.jpg\" alt>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<div class=\"align-center imageContainer\">\n\t\t\t\t<img class=\"circle-img\" src=\"@PROJECT_PATH@/assets/images/undraw-children-4rtb-1-843x606.png\" alt=\"Mobirise\">\n\t\t\t</div>\n\t\t\t<div class=\"column-2\">\n\t\t\t\t<div class=\"card card-right\" mbr-if=\"cardsAmount > 3\">\n\t\t\t\t\t<div class=\"item-image\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/neco-official-banner-160x135.jpg\" alt>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-box card-box2\">\n\t\t\t\t\t\t<h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle2\" data-app-selector=\".card-title\">NECO</h4>\n\t\t\t\t\t\t<p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Registered with NECO</p>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t<div class=\"card\" mbr-if=\"cardsAmount > 4\">\n\t\t\t\t\t<div class=\"item-image\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/napps-160x124.jpg\" alt>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-box card-box2\">\n\t\t\t\t\t\t<h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle2\" data-app-selector=\".card-title\">\n\t\t\t\t\t\t\tNAPPS</h4>\n\t\t\t\t\t\t<p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Member of National Association of Proprietors of Private Schools</p>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t<div class=\"card card-right\" mbr-if=\"cardsAmount > 5\">\n\t\t\t\t\t<div class=\"item-image\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/download-160x160.jpg\" alt>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-box card-box2\">\n\t\t\t\t\t\t<h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle2\" data-app-selector=\".card-title\">CAC</h4>\n\t\t\t\t\t\t<p class=\"mbr-text last-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Registered with Corporate Affairs Commission</p>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</section>",
